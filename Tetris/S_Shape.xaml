<UserControl x:Class="Tetris.S_Shape"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="20" Width="30">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Rectangle Grid.Column="0" Grid.Row="0" Stroke="Black" Fill="Red" />
        <Rectangle Grid.Column="1" Grid.Row="1" Stroke="Black" Fill="Red" />
        <Rectangle Grid.Column="1" Grid.Row="0" Stroke="Black" Fill="Red" />
        <Rectangle Grid.Column="2" Grid.Row="1" Stroke="Black" Fill="Red" />
    </Grid>
</UserControl>
